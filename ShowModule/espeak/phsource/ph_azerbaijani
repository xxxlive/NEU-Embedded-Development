
phoneme i
  vowel starttype #i endtype #i
  length 180
  FMT(vowel/ii_5)
endphoneme

phoneme e
  vowel starttype #e endtype #e
  length 200
  FMT(vowel/e_mid2)
endphoneme

phoneme &
  vowel starttype #e endtype #e
  length 200
  IF thisPh(isWordEnd) THEN
    FMT(vowel/&_3)
  ENDIF
  FMT(vowel/&)
endphoneme

phoneme y
  vowel starttype #u endtype #u
  length 200
  FMT(vowel/y)
endphoneme

phoneme W
  vowel starttype #@ endtype #@
  length 200
  FMT(vowel/y#_2)
endphoneme

phoneme u
  vowel starttype #u endtype #u
  length 180
  FMT(vowel/u_2)
endphoneme

phoneme o
  vowel starttype #o endtype #o
  length 200
  FMT(vowel/o_mid2)
endphoneme

phoneme a
  vowel starttype #a endtype #a
  length 200
  IF thisPh(isWordEnd) THEN
    FMT(vowel/a#)
  ENDIF
  FMT(vowel/aa_9)
endphoneme

phoneme @
  vowel starttype #@ endtype #@
  ipa É¯
  length 180
  FMT(vowel/u#_7)
endphoneme

